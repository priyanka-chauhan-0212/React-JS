          | if(i==1 || $i==10)
          | {
          | #{i}
          | }
        |                  }
    .suvery-suggestion
      span Not Likely
      span Very Likely
    |         }
    .suvery-divider
    | 		if (#{surveyDetails.skip_comment} == 0)
    .comment
      .comment-text #{surveyDetails.question}
      input#comment.comment-input(type='text' name='comment' placeholder='Write comment here')
    .comment
      .comment-owner-text Your Name
      input#name.comment-input(type='text' name='name' placeholder='Your name')
