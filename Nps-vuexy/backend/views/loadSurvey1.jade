style.
          .suvery-modal,
          .suvery-modal-thankyou {
          position: fixed;
          background-color: rgba(0, 85 ,137, .9);
          top: 0;
          right: 0;
          bottom: 0;
          left: 0;
          z-index: 999;
          visibility: hidden;
          opacity: 0;
          pointer-events: none;
          transition: all 0.3s;
          backdrop-filter: blur(3px);
          }
          .suvery-modal.open,
          .suvery-modal-thankyou.open {
          visibility: visible;
          opacity: 1;
          pointer-events: auto;
          }
          .suvery-modal .modal-main,
          .suvery-modal-thankyou .modal-main {
          position: absolute;
          top: 50%;
          left: 50%;
          transform: translate(-50%, -50%);
          max-width: 660px;
          width: 100%;
          padding: 2em;
          background-color: #fff;
          border-radius: 8px;
          }
          .suvery-modal .modal-main .nps-heading,
          .suvery-modal-thankyou .modal-main h2 {
          font-size: 22px;
          line-height: 28px;
          margin-bottom: 18px;
          }
          .suvery-modal-thankyou .modal-main {
          text-align: center;
          }
          .suvery-modal .modal-main .nps-desc {
          font-size: 16px;
          line-height: 18px;
          margin-bottom: 25px;
          display: inline-block;
          }
          .nps-rating.standard-suvery {
          display: flex;
          flex-wrap: wrap;
          align-items: center;
          justify-content: center;
          margin: 0 -10px;
          }
          .nps-rating.standard-suvery .btn-score:hover {
          opacity: 0.7 !important;
          }
          .nps-rating.standard-suvery .btn-score-inactive, .simple-emojis .btn-score-inactive{
          opacity: 0.6 !important;
          }
          .nps-rating.standard-suvery .number {
          font-size: 20px;
          font-weight: bold;
          width: 40px;
          min-width: 40px;
          height: 40px;
          display: flex;
          -webkit-box-pack: center;
          justify-content: center;
          -webkit-box-align: center;
          align-items: center;
          margin: 0 8px;
          /*background: rgb(99, 99, 99);
          color: white;
          border: 1px solid rgb(99, 99, 99);
          border-radius: 50%;*/
          cursor: pointer;
          {{$buttonBg}}
          {{$btnColor}}
          {{$btnBorder}}
          }
          .suvery-suggestion {
          display: flex;
          -webkit-box-pack: justify;
          justify-content: space-between;
          -webkit-box-align: center;
          align-items: center;
          margin-top: 10px;
          }
          .suvery-suggestion span {
          font-size: 14px;
          font-weight: 600;
          }
          .suvery-divider {
          margin: 30px 0;
          overflow: hidden;
          border-top: 1px solid #e9ecef;
          }
          .comment {
          margin-bottom: 40px;
          }
          .comment-text {
          color: rgb(24, 59, 86);
          font-size: 16px;
          line-height: 24px;
          font-weight: 600;
          margin-bottom: 16px;
          }
          .comment .comment-input {
          width: 100%;
          padding: 0.65rem 1rem;
          font-size: 1rem;
          font-weight: 400;
          line-height: 1.5;
          border: 1px solid #F3F6F9;
          border-radius: 0.42rem;
          background-color: #F3F6F9;
          color: #000;
          -webkit-box-sizing: border-box;
          box-sizing: border-box;
          }
          .btn-suvery-feedback {
          font-size: 14px;
          padding: 15px 40px;
          display: block;
          margin: 0 auto;
          border: none;
          border-radius: 8px !important;
          font-weight: 700 !important;
          background-color: rgb(188, 232, 151) !important;
          color: rgb(95, 121, 72) !important;
          outline: none !important;
          box-shadow: none !important;
          cursor: pointer;
          }
          .simple-emojis {
          display: flex;
          flex-wrap: wrap;
          margin-right: -15px;
          margin-left: -15px;
          }
          .simple-emojis .emoji-col {
          flex-basis: 0;
          flex-grow: 1;
          max-width: 100%;
          position: relative;
          width: 100%;
          padding-right: 15px;
          padding-left: 15px;
          }
          .simple-emojis .emoji-col .emoji-card {
          background: rgb(247, 248, 249);
          border-radius: 8px;
          height: 120px;
          display: flex;
          -webkit-box-pack: center;
          justify-content: center;
          -webkit-box-align: center;
          align-items: center;
          flex-direction: column;
          cursor: pointer;
          color: {{ $icon_color }};
          }
          .simple-emojis .emoji-col .emoji-card.active {
          background: 0% 0% no-repeat padding-box padding-box rgb(255, 255, 255);
          box-shadow: rgb(0 0 0 / 7%) 10px 10px 60px;
          }
          .simple-emojis .emoji-col .emoji-card .svg-inline--fa {
          display: inline-block;
          font-size: 30px;
          width: 1em;
          height: 1em;
          overflow: visible;
          vertical-align: -0.125em;
          }
          .simple-emojis .emoji-col .emoji-card .svg-inline--fa:hover {
          color: #726f6f;
          }
          .simple-emojis .emoji-card .emoji-text {
          font-size: 14px;
          font-weight: 600;
          color: {{ $text_color }};
          margin-top: 30px;
          }
          .modern-suvery {
          width: 100%;
          }
          .modern-suvery .bar {
          display: flex;
          justify-content: space-between;
          height: 5px;
          background-color: #dadfe3;
          margin: 30px 0;
          position: relative;
          }
          .modern-suvery .bar span {
          position: relative;
          top: 50%;
          transform: translateY(-50%);
          height: 20px;
          width: 4px;
          background-color: #dadfe3;
          cursor: pointer;
          }
          .modern-suvery .bar span:first-child,
          .modern-suvery .bar span:last-child {
          width: 40px;
          height: 40px;
          text-align: center;
          line-height: 40px;
          font-weight: 700;
          {{ $buttonBg }}
          {{ $btnColor }}
          {{ $btnBorder }}
          }
          .modern-suvery .bar span:first-child {
          background-color: #dadfe3;
          }
          .modern-suvery .bar span:last-child {
          background-color: #183b56;
          }
          .modern-suvery .bar {
          counter-reset: counter;
          }
          .modern-suvery .bar span {
          counter-increment: counter;
          }
          .modern-suvery .bar span.active:before {
          content: counter(counter);
          position: absolute;
          width: 40px;
          height: 40px;
          display: block;
          top: 50%;
          left: 50%;
          transform: translate(-50%, -50%);
          text-align: center;
          line-height: 40px;
          font-weight: 700;
          {{ $buttonBg }}
          {{ $btnColor }}
          {{ $btnBorder }}
          }
          .nps-close {
          position: absolute;
          right: 6px;
          top: 6px;
          width: 20px;
          height: 20px;
          opacity: 0.3;
          }
          .nps-close:hover {
          opacity: 1;
          }
          .nps-close:before, .nps-close:after {
          content: '';
          position: absolute;
          left: 6px;
          height: 20px;
          width: 2px;
          background-color: #333;
          }
          .nps-close:before {
          transform: rotate(45deg);
          }
          .nps-close:after {
          transform: rotate(-45deg);
          }
          button.close-thankyou {
          font-size: 14px;
          padding: 15px 40px;
          display: block;
          margin: 0 auto;
          border: none;
          border-radius: 8px !important;
          font-weight: 700 !important;
          background-color: #ff0000 !important;
          color: #fff !important;
          outline: none !important;
          box-shadow: none !important;
          cursor: pointer;
          }

.suvery-modal.open
